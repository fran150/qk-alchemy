{
  "name": "alchemy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "~3.3.4",
    "crossroads": "~0.12.0",
    "hasher": "~1.2.0",
    "requirejs": "~2.1.11",
    "requirejs-text": "~2.0.10",
    "knockout": "~3.3.0",
    "knockout-projections": "~1.1.0",
    "knockout-mapping": "~2.4.1",
    "blockui": "*",
    "accounting.js": "~0.4.1",
    "quark": "~0.5.4",
    "bootstrap-datepicker": "~1.4.0"
  },
  "main": [
	 "src/css/alchemy.css"
  ],
  "components": {
    "al-sidebar-search": "src/components/alchemy/sidebar-search/sidebar-search.js",
    "al-sidebar-item": "src/components/alchemy/sidebar-item/sidebar-item.js",
    "al-sidebar": "src/components/alchemy/sidebar/sidebar.js",
    "al-datepicker": "src/components/alchemy/datepicker/datepicker.js"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "src/bower_modules",
    "test",
    "src/app",
    "src/components/home-page",
    "src/components/nav-bar",
    "src/components/about-page",
    "src/css/styles.css",
    "src/index.html"
  ],
  "overrides": {
    "bootstrap": {
        "shims": ["jquery"],
        "components": {
            "bt-navbar": "dist/js/bootstrap.js",
            "bt-other": "dist/js/bootstrap.min.js"
        }
    },
    "crossroads": {
        "main": "dist/crossroads.min.js"
    },
    "js-signals": {
        "moduleName": "signals"
    },
    "accounting.js": {
        "main": "accounting.js",
        "moduleName": "accounting-js"

    },
    "knockout-mapping": {
        "main": "knockout.mapping.js",
        "shims": ["knockout", "jquery"]
    },
    "bootstrap-datepicker": {
        "main": [
            "dist/css/bootstrap-datepicker.css",
            "dist/js/bootstrap-datepicker.min.js"
        ],
        "fakes": {
            "bootstrap-datepicker-es": {
                "main": ["dist/locales/bootstrap-datepicker.es.min.js"],
                "shims": ["bootstrap-datepicker"]
            }
        }
    }
  }
}
